   "${INC_ROOT}/core/kernel/rexemu/src/arch/qdsp6",
   "${INC_ROOT}/core/kernel/rexemu/src",
   "${INC_ROOT}/core/kernel/dl_pager/inc",
   "${INC_ROOT}/core/api/memory",
