   "${INC_ROOT}/core/api/kernel/rex/os/qurt",
   "${INC_ROOT}/core/api/kernel/rte",
   "${INC_ROOT}/core/api/kernel/quartz",
   "${INC_ROOT}/core/api/kernel/qurt",
   "${INC_ROOT}/core/api/kernel/posix",
   "${INC_ROOT}/core/api/kernel/qube",
